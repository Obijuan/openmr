<!-- Head of the Y1 module
     It is located in the head of the snake
     The reference frame is located in the shaft
     It can yaw right and left
-->

<Kinbody name="khead">
   <Body name="Head" type="dynamic">
      <mass type="sphere">
        <total>0.025</total>
        <radius>0.018</radius>
      </mass>
      <Geom type="trimesh">
        <Render>Y1-head.iv 0.001</Render>
        <data>Y1-head.iv 0.001</data>
        <rotationaxis>0 1 0 -90</rotationaxis>
        <translation>0.007 0.018 -0.007</translation>
      </Geom>
    </Body>
</Kinbody>

